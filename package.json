{
  "name": "@metamask/tx-categorize",
  "version": "0.7.1",
  "description": "Transaction categorization library for blockchain transactions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Consensys Software Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/tx-categorize.git"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "transaction",
    "categorization",
    "defi",
    "nft",
    "metamask",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "categorize": "yarn build && node ./scripts/categorize.js",
    "list:metrics": "yarn build && node ./scripts/listMetrics.js",
    "test": "jest --passWithNoTests",
    "test:cov": "jest --ci --coverage",
    "release": "standard-version",
    "tscheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/i18next": "^13.0.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^18.19.45",
    "@types/seedrandom": "^3",
    "jest": "^29.7.0",
    "nock": "^13.2.4",
    "seedrandom": "^3.0.5",
    "standard-version": "^9.3.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "bloom-filters": "^3.0.1",
    "i18next": "^24.2.3",
    "i18next-fs-backend": "^2.6.0"
  }
}
